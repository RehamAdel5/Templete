    @await Html.PartialAsync("_CallToAction")
</section> *@
<section id="portfolio" class="portfolio section"> 
    @await Html.PartialAsync("~/Views/Site/Home/_Projects.cshtml", Model.ProjectList)
</section>
<section id="team" class="team section">
    @await Html.PartialAsync("~/Views/Site/Home/_Team.cshtml", Model.TeamList)
</section>
<section id="pricing" class="pricing section light-background">
    @await Html.PartialAsync("~/Views/Site/Home/_Pricing.cshtml", Model.FeaturesList)
</section>
<section id="testimonials" class="testimonials section">
    @await Html.PartialAsync("~/Views/Site/Home/_Testimonials.cshtml", Model.TestimonialList)
</section>
<section id="faq-2" class="faq-2 section light-background">
    @await Html.PartialAsync("~/Views/Site/Home/_AskedQuestions.cshtml", Model.AskedQuestionList)
</section>
<section id="contact" class="contact section">
    @await Html.PartialAsync("~/Views/Site/Home/_ContactUs.cshtml", Model.ContactUs)
</section>