@using Domain.ViewModels;
@model IndexViewModel
@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<section id="hero" class="hero section dark-background">
    @await Html.PartialAsync("~/Views/Site/Home/_CompanyInformation.cshtml")
</section>
<section id="clients" class="clients section light-background">
    @await Html.PartialAsync("~/Views/Site/Home/_HorizontalSlider.cshtml", Model.HorizontalSliderList)
</section>
<section id="about" class="about section">
    @await Html.PartialAsync("~/Views/Site/Home/_About.cshtml", Model.AboutList)
</section>
<section id="why-us" class="section why-us light-background" data-builder="section">
    @await Html.PartialAsync("~/Views/Site/Home/_WhyUs.cshtml", Model.WhyUsList)
</section>
<section id="skills" class="skills section">
    @await Html.PartialAsync("~/Views/Site/Home/_Skills.cshtml", Model.SkillsList)
</section>
<section id="services" class="services section light-background">
    @await Html.PartialAsync("~/Views/Site/Home/_Services.cshtml", Model.ServicesList)
</section>
